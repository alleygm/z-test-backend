{% extends 'base/base_center.html.twig' %}

{% block content %}
	<h1 class="text-center">Регистрация</h1>

	{{ form_errors(form) }}
	{{ form_start(form) }}
		{{ form_row(form.username) }}
		{{ form_row(form.plainPassword) }}
		{{ form_row(form.submit) }}
		<a href="{{path('auth')}}" class="default-btn" data-turbo-action="replace">назад</a>
	{{ form_end(form) }}

{% endblock %}
